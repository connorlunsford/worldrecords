<h3>Insert Data Into "Customers"</h3>
<form action="flip3.engr.oregonstate.edu:7753/customers_insert" method="GET">
    <label for="f_name">First Name: </label>
    <input type="text" id="f_name"><br>
    <label for="l_name">Last Name: </label>
    <input type="text" id="l_name"><br>
    <label for="email">Email: </label>
    <input type="text" id="email"><br>
    <input type="submit" id="customers">
</form>

<div>
    <table>
        <thead>
            <tr id="head">
                <th>Customer ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Update</th>
                <th>Delete</th>
            </tr>
        </thead>
        <tbody>
          {{#each results}}
            <tr>
                <td>{{this.cid}}</td>
                <td>{{this.f_name}}</td>
                <td>{{this.l_name}}</td>
                <td>{{this.email}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>

